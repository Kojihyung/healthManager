<script>
  window.addEventListener("load", function () {
    var chart = bb.generate({
      data: {
        columns: [["sample", 30, 200, 100, 400, 150, 250]],
      },
      regions: [
        {
          start: 0,
          end: 1,
        },
        {
          start: 2,
          end: 4,
          class: "foo",
        },
      ],
      bindto: "#styleForRegion",
    });
  });
</script>

<style>
  #styleForRegion .bb-region-0 {
    fill: red;
  }
  #styleForRegion .bb-region.foo {
    fill: green;
  }
</style>

